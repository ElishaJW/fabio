Package: fabio
Type: Package
Title: Forestry and Agriculture Biomass Input-Output Tables
Version: 0.1.0.9000
Authors@R: c(
    person("Nikolas", "Kuschnig", role = c("aut", "cre")),
    person("Martin", "Bruckner", role = c("aut", "cre"), comment = c(ORCID = "0000-0002-1405-7951"))
